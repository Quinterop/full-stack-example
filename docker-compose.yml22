version: '1'
services:
  db:
    image: postgres
    ports:
      - "5432:5432"
    volumes:
      - db-data:/var/lib/postgresql/data
  backend:
    build:
      context: ./back/fullstacktest
    depends_on:
      - db
    ports:
      - "8080:8080"    
  frontend:
    build:
      context: ./frontt/fullstapp
    ports:
      - "4200:4200"
volumes:
  db-data: